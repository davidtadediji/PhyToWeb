{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "CaseDetails": {
      "type": "object",
      "properties": {
        "ocdCaseTypeId": { "type": "string" },
        "ocdCaseSubTypeId": { "type": "string" },
        "ocdWorkflowId": { "type": "string" },
        "ocdAssignerId": { "type": "string" },
        "ocdAssignedDate": { "type": "string", "format": "date-time" },
        "ocdAssigneeId": { "type": "string" },
        "ocdStatusId": { "type": "string" },
        "ocdActionId": { "type": "string" },
        "ocdIsEditable": { "type": "boolean" }
      },
      "required": [
        "ocdCaseTypeId",
        "ocdCaseSubTypeId",
        "ocdAssignerId",
        "ocdAssignedDate",
        "ocdAssigneeId",
        "ocdStatusId",
        "ocdActionId",
        "ocdIsEditable"
      ]
    },
    "Product": {
      "type": "object",
      "properties": {
        "tempId": { "type": "string" },
        "productName": { "type": "string" },
        "version": { "type": "string" },
        "productType": { "type": "string" },
        "launchDate": { "type": "string", "format": "date" },
        "category": { "type": "string" },
        "website": { "type": "string", "format": "uri" },
        "productDescription": { "type": "string" },
        "logo": { "type": "string", "format": "uri" }
      },
      "required": [
        "productName",
        "version",
        "productType",
        "launchDate",
        "category",
        "website",
        "productDescription",
        "logo"
      ]
    },
    "Address": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "tempId": { "type": "string" },
          "address1": { "type": "string" },
          "address2": { "type": "string" },
          "addressType": { "type": "string" },
          "country": { "type": "string" },
          "postalCode": { "type": "string" },
          "startDate": { "type": "string", "format": "date-time" },
          "endDate": { "type": "string", "format": "date-time" }
        },
        "required": [
          "address1",
          "addressType",
          "country",
          "postalCode",
          "startDate",
          "endDate"
        ]
      }
    },
    "Contact": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "tempId": { "type": "string" },
          "contactType": { "type": "string" },
          "contactValue": { "type": "string" },
          "isPrimary": { "type": "boolean" }
        },
        "required": [
          "contactType",
          "contactValue",
          "isPrimary"
        ]
      }
    },
    "ContactPerson": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "tempId": { "type": "string" },
          "firstName": { "type": "string" },
          "lastName": { "type": "string" },
          "assignedPerson": { "type": "string" },
          "address": { "type": "string" },
          "organisation": { "type": "string" },
          "role": { "type": "string" }
        },
        "required": [
          "firstName",
          "lastName",
          "assignedPerson",
          "address",
          "organisation",
          "role"
        ]
      }
    },
    "FinancialInformation": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "tempId": { "type": "string" },
          "dateFrom": { "type": "string", "format": "date" },
          "dateTo": { "type": "string", "format": "date" },
          "accountType": { "type": "string" },
          "amount": { "type": "number" },
          "address": { "type": "string" }
        },
        "required": [
          "accountType",
          "amount",
          "address"
        ]
      }
    }
  },
  "required": [
    "CaseDetails",
    "Product",
    "Address",
    "Contact",
    "ContactPerson",
    "FinancialInformation"
  ]
}
